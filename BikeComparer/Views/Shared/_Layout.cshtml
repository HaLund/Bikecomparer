    <link rel="apple-touch-icon" href="../Images/Icons/icon196x196.png" />
    <link rel="stylesheet" href="~/css/StyleSheet1.css" asp-append-version="true" />
    <link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css" />

