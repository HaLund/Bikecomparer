@model BikeComparer.Models.BikeComparisonViewModel

@{
    ViewData["Title"] = "BikeComparison";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<head>
    <link href="~/Content/bikes.css" rel="stylesheet" asp-append-version="true" />
    <link href="~/Content/SiteBase.css" rel="stylesheet" asp-append-version="true" />
    <meta charset="utf-8" />
    <meta name="description" content="Detaljerade specifikationer om valda motorcyklar | Bikecomparer" />
    <meta name="keywords" content="hojar, motorcyklar, motorcykel, bikelexicon" />
</head>
<form name="BikeFormComparison" asp-controller="Home" asp-action="Comparison" method="get" class="BikesContentRight areaMargin shadow">
    <table class="">
        <tr>
            <td style="height:auto;">
            </td>
            @for (int i = 0; i < Model.BikeComparisonList.Count; i++)
            {
                <td style="width250px;alignCenter">
                    <img src="@Model.BikeComparisonList[i].ImgPath" alt="@Model.BikeComparisonList[i].imgName" />
                </td>
            }
        </tr>
        <tr class="tblCompareRowOne">
            <td class="headerStyle">
                Modell
            </td>
            @for (int i = 0; i < Model.BikeComparisonList.Count; i++)
            {
                <td class="width250px" style="width250px;">
                    @Model.BikeComparisonList[i].FullName
                </td>
            }
        </tr>
        <tr class="tblCompareRowOne">
            <td class="headerStyle" >
                År
            </td>
            @for (int i = 0; i < Model.BikeComparisonList.Count; i++)
            {
                <td class="width250px" style="width250px;">
                    @Model.BikeComparisonList[i].FirstYear
                </td>
            }
        </tr>
        <tr>
            <td class="headerStyle">
                Kategori
            </td>
            @for (int i = 0; i < Model.BikeComparisonList.Count; i++)
            {
                <td class="width250px" style="width250px;">
                    @Model.BikeComparisonList[i].BikeCategory
                </td>
            }
        </tr>
        <tr>
            <td class="headerStyle">
                Motor
            </td>
            @for (int i = 0; i < Model.BikeComparisonList.Count; i++)
            {
                <td class="width250px" style="width250px;">
                    @Model.BikeComparisonList[i].Engine
                </td>
            }
        </tr>
        <tr>
            <td class="headerStyle">
                Ram
            </td>
            @for (int i = 0; i < Model.BikeComparisonList.Count; i++)
            {
                <td class="width250px" style="width250px;">
                    @Model.BikeComparisonList[i].Frame
            </td>
            }
        </tr>
        <tr>
            <td class="headerStyle">
                Fjädring fram
            </td>
            @for (int i = 0; i < Model.BikeComparisonList.Count; i++)
            {
                <td class="width250px" style="width250px;">
                    @Model.BikeComparisonList[i].FrontSuspension
                </td>
            }
        </tr>
        <tr>
            <td class="headerStyle">
                Fjädring Bak
            </td>
            @for (int i = 0; i < Model.BikeComparisonList.Count; i++)
            {
                <td class="width250px" style="width250px;">
                    @Model.BikeComparisonList[i].RearSuspension
                </td>
            }
        </tr>
        <tr>
            <td class="headerStyle">
                Bromsar fram
            </td>
            @for (int i = 0; i < Model.BikeComparisonList.Count; i++)
            {
                <td class="width250px" style="width250px;">
                    @Model.BikeComparisonList[i].FrontBrakes
                </td>
            }
        </tr>
        <tr>
            <td class="headerStyle">
                Bromsar bak
            </td>
            @for (int i = 0; i < Model.BikeComparisonList.Count; i++)
            {
                <td class="width250px" style="width250px;">
                    @Model.BikeComparisonList[i].RearBrakes
                </td>
            }
        </tr>
        <tr>
            <td class="headerStyle">
                Däck fram
            </td>
            @for (int i = 0; i < Model.BikeComparisonList.Count; i++)
            {
                <td class="width250px" style="width250px;">
                    @Model.BikeComparisonList[i].FrontTyre
                </td>
            }
        </tr>
        <tr>
            <td class="headerStyle">
                Däck bak
            </td>
            @for (int i = 0; i < Model.BikeComparisonList.Count; i++)
            {
                <td class="width250px" style="width250px;">
                    @Model.BikeComparisonList[i].RearTyre
