                {
                <td class="width250px" style="width250px;">
                    @Model.BikeComparisonList[i].EngineMaps
                </td>
                }
        </tr>
        <tr>
            <td class="headerStyle">
                Wheelie control
            </td>
                @for (int i = 0; i < Model.BikeComparisonList.Count; i++)
                {
                <td class="width250px" style="width250px;">
                    @Model.BikeComparisonList[i].WheelieControl
                </td>
                }
        </tr>
        <tr>
            <td class="headerStyle">
                Drivning
            </td>
                @for (int i = 0; i < Model.BikeComparisonList.Count; i++)
                {
