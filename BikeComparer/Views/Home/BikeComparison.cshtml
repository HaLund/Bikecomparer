                    @Model.BikeComparisonList[i].Gearbox
                </td>
            }
        </tr>
        <tr>
            <td class="headerStyle">
                Drivning
            </td>
            @for (int i = 0; i < Model.BikeComparisonList.Count; i++)
            {
                <td class="width250px" style="width250px;">
                    @Model.BikeComparisonList[i].FinalDrive
                </td>
            }
        </tr>
        <tr>
            <td class="headerStyle">
                Koppling
            </td>
